{
    "name": "miracron",
    "image": "mcr.microsoft.com/vscode/devcontainers/python:3",
    "containerEnv": {
        "TZ": "Asia/Tokyo"
    },
    "extensions": [
        "ms-python.python",
        "bungcip.better-toml",
        "EditorConfig.EditorConfig"
    ],
    "postCreateCommand": "pip3 install --requirement .devcontainer/requirements.txt"
}
